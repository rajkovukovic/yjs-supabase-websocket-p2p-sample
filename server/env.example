# Supabase Configuration
SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Server Configuration
HOCUSPOCUS_PORT=1234
Y_WEBRTC_SIGNALING_PORT=4445
NODE_ENV=development

# Optional: CORS Configuration
CORS_ORIGIN=*

# Optional: Database Table Names (defaults to standard names)
TABLE_DOCUMENTS=documents
TABLE_DOCUMENT_UPDATES=document_updates
TABLE_DOCUMENT_SNAPSHOTS=document_snapshots

# Optional: WebRTC room password (optional) - adds security to P2P rooms
# All users must use the same password to connect in the same room
Y_WEBRTC_PASSWORD=your-secure-password

